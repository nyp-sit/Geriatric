<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity$PlaceholderFragment">


    <ImageView
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="29dp"
        android:src="@drawable/icon"
        android:layout_x="38dp"
        android:layout_y="45dp" />

    <TextView
        android:layout_width="145dp"
        android:layout_height="42dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Silver Vitality"
        android:id="@+id/headerText"
        android:layout_x="117dp"
        android:layout_y="91dp"
        android:fontFamily="sans-serif-light" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/btn_medbook"
        android:layout_x="0dp"
        android:layout_y="260dp"
        android:background="#ff26bbff"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/btn_safety"
        android:layout_x="112dp"
        android:layout_y="260dp"
        android:background="#fff3014e" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/btn_appointment"
        android:layout_x="224dp"
        android:layout_y="260dp"
        android:background="#ff2dacbf" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/btn_vitality"
        android:layout_x="0dp"
        android:layout_y="370dp"
        android:background="#ffe41d08" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/btn_about"
        android:layout_x="112dp"
        android:layout_y="370dp"
        android:background="#ffec8800" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/btn_settings"
        android:layout_x="224dp"
        android:layout_y="370dp"
        android:background="#ffe671b8" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/img_medbook"
        android:layout_x="40dp"
        android:layout_y="260dp"
        android:src="@drawable/hardbook" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Medbook"
        android:id="@+id/textView"
        android:textColor="#ffffff"
        android:layout_x="3dp"
        android:fontFamily="sans-serif"
        android:layout_y="342dp" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/img_gps"
        android:layout_x="150dp"
        android:layout_y="260dp"
        android:src="@drawable/gps"
        android:adjustViewBounds="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Safety on the go"
        android:id="@+id/textView2"
        android:layout_x="113dp"
        android:fontFamily="sans-serif"
        android:textColor="#ffffff"
        android:layout_y="342dp" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/img_calender"
        android:layout_x="260dp"
        android:layout_y="260dp"
        android:src="@drawable/calender" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Appointment \n Buddy"
        android:id="@+id/textView3"
        android:layout_x="225dp"
        android:fontFamily="sans-serif"
        android:textColor="#ffffff"
        android:layout_y="330dp" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/image_vit"
        android:layout_x="50dp"
        android:layout_y="380dp"
        android:focusableInTouchMode="true"
        android:src="@drawable/pulse" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vitality Status"
        android:id="@+id/textView4"
        android:layout_x="3dp"
        android:fontFamily="sans-serif"
        android:textColor="#ffffff"
        android:layout_y="450dp" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/img_about"
        android:layout_x="150dp"
        android:layout_y="370dp"
        android:focusableInTouchMode="false"
        android:src="@drawable/heart" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About Us"
        android:id="@+id/textView5"
        android:layout_x="113dp"
        android:fontFamily="sans-serif"
        android:textColor="#ffffff"
        android:layout_y="450dp" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageView2"
        android:layout_x="260dp"
        android:layout_y="370dp"
        android:src="@drawable/gear" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Settings"
        android:id="@+id/textView6"
        android:layout_x="225dp"
        android:fontFamily="sans-serif"
        android:textColor="#ffffff"
        android:layout_y="450dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Live smarter and safer"
        android:id="@+id/textView7"
        android:layout_x="90dp"
        android:layout_y="130dp"
        android:fontFamily="sans-serif-light"/>

</AbsoluteLayout>
